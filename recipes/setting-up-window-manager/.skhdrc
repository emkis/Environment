# Fast application launcher
hyper - q : ~/bin/ide-manager --open
hyper - w : open -a "Warp"
hyper - e : open -a "Notion"
hyper - r : open -a "TickTick"

hyper - a : open -a "Arc"
hyper - s : open -a "YouTube Music"
hyper - d : open -a "Google Chrome"
hyper - f : open -a "Simulator"

# Bluetooth controls
hyper - b : ~/bin/toggle-bluetooth
